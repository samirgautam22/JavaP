package com.project.grocery.model;

import java.io.Serializable;

import javax.persistence.Lob;

/**
 * @author:Samir Gautam
 * @Version:1.0
 * @Date:Aug 27, 2018
 * 
 */
@SuppressWarnings("serial")
public class Contact implements Serializable {
	
	private Long id;
	private String name;
	private String email;
	@Lob
	private String message;

}
